@{
  Layout = "_Layout";
}

@model ParksClient.Models.Park;

<div class="jumbotron">
    <h2 class="display-4">Add Park</h2>
</div>

<div class='content'>
<form asp-controller="Parks" asp-action="CreatePark" method="post">
    <br>
    @Html.LabelFor(model => model.Name)
    @Html.TextBoxFor(model => model.Name)
    <br>
    @Html.LabelFor(model => model.IsNational)
    @Html.TextBoxFor(model => model.IsNational)
    <br>
    @Html.LabelFor(model => model.StateId)
    @Html.TextBoxFor(model => model.StateId)
    <br>
    <input type="submit" value="Add Destination" class="btn btn-secondary" />
</form>
</div>