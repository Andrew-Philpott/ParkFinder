@{
  Layout = "_Layout";
}

@using ParksClient.Models;
@model IEnumerable<ParksClient.Models.Park>

<div class="jumbotron">
    <h1>Search Results</h1>
</div>

<div class='content'>
@if(Model.Any())
{
  @foreach (Park park in @Model)
  {
    <h2>@Html.ActionLink($"{park.Name}",  "Details", new { id = park.Id })</h2>
  }
}
else
{
  <h3>No Parks available</h3>
}
</div>
<div class="links">
  <a href="/parks">Back to parks</a>
  <a href="/home">Home</a>
</div>
